{
  "env": {
    "development": {
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "../"
            ],
            "alias": {
              "~": "../",
              "actions": "./src/actions",
              "api": "./src/api",
              "components": "./src/components",
              "constants": "./src/constants",
              "hoc": "./src/hoc",
              "images": "./src/images",
              "layouts": "./src/layouts",
              "reducers": "../reducers",
              "selectors": "./src/selectors",
              "styles": "./src/styles",
              "utils": "./src/utils",
              "views": "./src/views"
            }
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "src"
            ],
            "alias": {
              "~": "./src",
              "actions": "./src/actions",
              "api": "./src/api",
              "components": "./src/components",
              "constants": "./src/constants",
              "hoc": "./src/hoc",
              "images": "./src/images",
              "layouts": "./src/layouts",
              "reducers": "./src/reducers",
              "selectors": "./src/selectors",
              "styles": "./src/styles",
              "utils": "./src/utils",
              "views": "./src/views"
            }
          }
        ]
      ]
    }
  }
}